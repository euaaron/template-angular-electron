{
  "name": "ngx-electro-bro",
  "productName": "Electro Bro - Angular",
  "description": "A simple multi-platform web browser made with Electron + Angular Ivy",
  "homepage": "https://github.com/euaaron/angular-browser",
  "version": "0.0.0",
  "main": "./dist/electron/electron.js",
  "author": {
    "name": "Aaron Carneiro",
    "email": "hello@aaroncarneiro.com",
    "url": "https://aaroncarneiro.com"
  },
  "scripts": {
    "ng": "ng",
    "watch": "tsc -w",
    "electron:build": "tsc --build electron",
    "build": "ng build && yarn electron:build ",
    "build:prod": "ng build --configuration=production && yarn electron:build ",
    "start": "electron .",
    "start:ng": "ng serve",
    "start:dev": "concurrently 'ng serve' 'yarn watch' 'yarn start'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pack:all": "yarn build:prod && npx electron-packager --out='packages' --all .",
    "pack:win": "yarn build:prod && npx electron-packager --out='packages' --platform=win32 --arch=x64 --dir=dist ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "angular-feather": "^6.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular-eslint/builder": "4.3.0",
    "@angular-eslint/eslint-plugin": "4.3.0",
    "@angular-eslint/eslint-plugin-template": "4.3.0",
    "@angular-eslint/schematics": "4.3.0",
    "@angular-eslint/template-parser": "4.3.0",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "concurrently": "^6.2.2",
    "electron": "^15.0.0",
    "electron-packager": "^15.4.0",
    "eslint": "^7.6.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "~4.1.5"
  },
  "optionalDependencies": {
    "husky": "^7.0.2"
  }
}
